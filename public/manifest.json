{
  "manifest_version": 3,
  "name": "Quick Typing Test",
  "description": "A typing test that runs in the browser popup.",
  "version": "1.1.0",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "16": "icon.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": []
}